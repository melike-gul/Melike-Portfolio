{
  "name": "melike-portfolio",
  "version": "2.0.0",
  "author": "Melike GUL",
  "private": false,
  "scripts": {
    "css": "postcss assets/styles/main.css -o assets/styles/main.min.css --verbose",
    "server-reload": "browser-sync start --config './browser-sync-config.js'",
    "develop": "concurrently 'npm:css -- --watch' 'npm:server-reload'",
    "build": "your-build-command --output public",
    "format": "npx prettier ./**/*.html"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "dependencies": {
    "browser-sync": "^2.27.7",
    "concurrently": "^7.0.0",
    "cssnano": "^5.0.17",
    "postcss-cli": "^9.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8"
  }
}